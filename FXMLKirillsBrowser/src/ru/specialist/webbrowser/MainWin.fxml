<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.web.WebView?>

<BorderPane prefHeight="700" prefWidth="1100" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.specialist.webbrowser.MainWinController">
   <center>
      <WebView fx:id="browser" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
   <top>
      <BorderPane prefHeight="32.0" BorderPane.alignment="CENTER">
         <right>
            <Button mnemonicParsing="false" onAction="#GO" text="Search" BorderPane.alignment="CENTER" />
         </right>
         <center>
            <TextField fx:id="txtURL" prefHeight="22.0" prefWidth="500.0" promptText="Enter your desired URL, starting with http://" BorderPane.alignment="CENTER" />
         </center>
      </BorderPane>
   </top>
</BorderPane>
